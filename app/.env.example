# Copy to `.env.local` and fill values.
#
# Local dev:
# - Start: `pnpm db:start`
# - Then run: `pnpm db:status` and copy URL/keys below.

# Public (browser)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Server only (never expose to browser)
SUPABASE_SERVICE_ROLE_KEY=

# Type generation
# - Remote: set SUPABASE_PROJECT_ID, then `pnpm db:gen`
# - Local: `pnpm db:gen -- --local`
SUPABASE_PROJECT_ID=
SUPABASE_SCHEMA=public

# Email (Resend) - server only
RESEND_API_KEY=
RESEND_FROM_EMAIL=


# SMS/알림톡 (Solapi) - server only, P2+
SOLAPI_API_KEY=
SOLAPI_API_SECRET=
SOLAPI_SENDER_PHONE=
# SOLAPI_KAKAO_PFID=  # 카카오 알림톡 발신프로필 ID (알림톡 사용 시)

# Site URL (for SEO metadata)
NEXT_PUBLIC_SITE_URL=https://medihub.kr

# Sentry - error tracking
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=  # for source map uploads

# TossPayments - server only
# API 개별 연동 키 (테스트: test_sk_..., 라이브: live_sk_...)
TOSS_PAYMENTS_SECRET_KEY=
# 웹훅 서명 검증용 보안 키
TOSS_PAYMENTS_WEBHOOK_SECRET=
# API 개별 연동 클라이언트 키 (브라우저 결제 요청용)
NEXT_PUBLIC_TOSS_PAYMENTS_CLIENT_KEY=
